# AnchorLayout
# anchor_x: 'right', 'center', 'left'
# anchor_y: 'top', 'center', bottom'

<Wid_Alfa>:
	canvas:
		Color:
			rgb: 1,1,1
		Rectangle:
			size: self.size 
			pos: self.pos 
	AnchorLayout:
		size_hint: (None, None)
		size: 100, 100
		canvas:
			Color:
				rgb: 0,0,0
			Rectangle:
				size: self.size 
				pos: self.pos

